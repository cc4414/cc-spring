<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.cc4414.spring.sys.mapper.DictMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap"
		type="cc.cc4414.spring.sys.entity.Dict">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="code" property="code" />
		<result column="remarks" property="remarks" />
		<result column="deleted" property="deleted" />
		<result column="disabled" property="disabled" />
		<result column="gmt_create" property="gmtCreate" />
		<result column="creator_id" property="creatorId" />
		<result column="creator_name" property="creatorName" />
		<result column="gmt_modified" property="gmtModified" />
		<result column="modifier_id" property="modifierId" />
		<result column="modifier_name" property="modifierName" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		id, name, code, remarks, deleted, disabled, gmt_create, creator_id,
		creator_name, gmt_modified, modifier_id, modifier_name
	</sql>

</mapper>
